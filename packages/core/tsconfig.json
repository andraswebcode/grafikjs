{
	"compilerOptions": {
		"outDir": "./dist/",
		"module": "es6",
		"target": "es5",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "Node",
		"strict": true,
		"strictPropertyInitialization": false,
		"noImplicitAny": false,
		"baseUrl": ".",
		"paths": {
			"*": ["src/*"]
		}
	},
	"include": ["./src/**/*.ts"]
}
