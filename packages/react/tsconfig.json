{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "strict": true,
    "strictPropertyInitialization":false,
    "noImplicitAny":false,
    "esModuleInterop":true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}
